{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "plugin:@getify/proper-arrows/getify-says",
    "plugin:@getify/proper-ternary/getify-says"
  ],
  "rules": {
    "no-console": "warn"
  },
  "plugins": ["import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
